            <div class="wrapper wrapper-content">

                <div class="row">
                    <div class="col-md-12">
                        <div class="ibox float-e-margins">
                            <div class="ibox-title">
                                <h5>Proveedores</h5>
                                <div class="ibox-tools">
                                    <a class="collapse-link">
                                        <i class="fa fa-chevron-up"></i>
                                    </a>



                                </div>
                            </div>
                            <div class="ibox-content">

                                <div class="row">

                                    <div class="col-md-12">
                                        <div class="row">
                                            <br>
                                            <div class="col-md-3">
                                                <table class="table">
                                                    <tr>
                                                        <td>
                                                            <strong>Cliente:</strong>
                                                        </td>
                                                        <td>
                                                            <%=data.name%>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <strong>Fecha:</strong>
                                                        </td>
                                                        <td>
                                                            <%=data.date%>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <strong>Referencia:</strong>
                                                        </td>
                                                        <td>
                                                            <%=data.reference%>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <strong>Creado por:</strong>
                                                        </td>
                                                        <td>
                                                            <%=data.user%>
                                                        </td>
                                                    </tr>

                                                </table>




                                            </div>
                                            <div class="col-md-3" id="formSave">
                                                <div class="form-group">
                                                    <label for="code">Número de serie:</label>
                                                    <input autofocus type="text" class="form-control" required id="code2" name="code" style="width: 100%" placeholder="Ingresar número de serie">
                                                    <br>
                                                    <label for="code">Obervación: (opcional)</label>
                                                    <textarea autofocus style="resize: none" class="form-control" name="observation" id="observation" rows="2" placeholder="Ingrese una observación"></textarea>
                                                    <br>


                                                    <input id="voucher" type="hidden" value="<%=bill%>">
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <button id="closeVoucher" class="btn btn-danger">Cerrar salida</button>
