                    <li><a href="/provider"><i class="fa fa-truck"></i> <span class="nav-label">Proveedores</span></a></li>


                </ul>

            </div>
        </nav>

        <div id="page-wrapper" class="gray-bg">
            <div class="row border-bottom">
                <nav class="navbar navbar-static-top  " role="navigation" style="margin-bottom: 0">
                    <div class="navbar-header">
                        <a class="navbar-minimalize minimalize-styl-2 btn btn-primary " href="#"><i class="fa fa-bars"></i> </a>
                        <form role="search" class="navbar-form-custom" action="search_results.html">
                            <div class="form-group">

                            </div>
                        </form>
                    </div>
                    <ul class="nav navbar-top-links navbar-right">
                        <li>
                            <span class="m-r-sm text-muted welcome-message">Bienvenido al sistema de inventario</span>
                        </li>

                        <li>
                            <a href="/logout">
                        <i class="fa fa-sign-out"></i> Log out
                    </a>
                        </li>
                    </ul>

                </nav>
            </div>


            <div class="wrapper wrapper-content">

                <div class="row">
                    <div class="col-md-12">
                        <div class="ibox float-e-margins">
                            <div class="ibox-title">
                                <h5>Inicio</h5>
                                <div class="ibox-tools">
                                    <a class="collapse-link">
                                                                <i class="fa fa-chevron-up"></i>
                                                            </a>



                                </div>
                            </div>
                            <div class="ibox-content">

                                <div class="row">

                                    <div class="col-md-9 well">


                                        <div class="row">

                                            <form id="formsave">
                                                <fieldset>


                                                    <div class="col-md-4">
                                                        <label for="provider">Seleccionar proveedor</label>
                                                        <input required style="width: 100%" class="form-control" type="text" id="provider" name="provider">
                                                    </div>
                                                    <div class="col-md-4">
                                                        <label for="date">Seleccionar fecha de ingreso</label>
                                                        <input required style="width: 100%" class="form-control" type="date" id="date" name="date">
                                                    </div>
                                                    <div class="col-md-4">
                                                        <label for="type">Tipo de documento</label>
                                                        <select required class="form-control" name="type" id="type">
                                                                                                                        <option value="1">FACTURA</option>
                                                                                                                        <option value="2">ACTA</option>
                                                                                                                        <option value="3">GUÍA DE REMISIÓN</option>
                                                                                                                    </select>

                                                    </div>
                                                    <div class="col-md-4">
                                                        <label for="reference">Referencia</label>
                                                        <input required class="form-control" type="text" id="reference" name="reference">
                                                        <input required class="form-control" type="hidden" id="user" name="user" value="<%=user.id%>">
                                                    </div>

                                                    <div class="col-md-4">

                                                        <br> <button class="btn btn-primary" onclick="saveBill()" type="button">Guardar orden de ingreso</button>

                                                    </div>
                                                </fieldset>
                                            </form>
                                        </div>




                                    </div>
                                    <div class="col-md-3 well">
                                        <a class="btn btn-link" href="../PLANTILLA.csv"> Descargar plantilla</a><br><br>
                                        <form class="form-horizontal ">

                                            <fieldset>
                                                <label for="csvFileInput"> <strong>Seleccionar archivo CSV:</strong>
                                                                                                                     </label>
                                                <input type="file" class="" id="csvFileInput" onchange="handleFiles(this.files)" accept=".csv"><br>
                                                <button type="button" class="btn btn-primary" onclick="chargeData()">Enviar datos</button>
                                                <i id="spinner" class="fa fa-spinner fa-spin fa-2x" aria-hidden="true"></i>

                                            </fieldset>
                                        </form>
                                    </div>
                                    <div id="output" style="height: 150; background-color: white"></div>
                                </div>

                            </div>
                        </div>
                    </div>






                </div>


            </div>
            <div class="footer">
                <div class="pull-right">
                    Cineto Telecomunicaciones S.A.
                </div>
                <div>
                    <strong>Copyright</strong> Cineto Telecomunicaciones S.A. &copy; 2017
                </div>
            </div>

        </div>

