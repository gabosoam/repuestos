                                <li><a href="login.html">Logout</a></li>
                            </ul>
                        </div>
                        <div class="logo-element">
                            IN+
                        </div>
                    </li>
                    <li><a href="/"><i class="fa fa-home"></i> <span class="nav-label">Existencias</span></a></li>
                    <li class="active">
                        <a href="/bill"><i class="fa fa-arrow-right"></i> <span class="nav-label">Ingresos</span> <span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level">
                            <li><a href="/bill">Ingreso normal</a></li>
                            <li class="active"><a href="/lotes">Ingreso por lotes</a></li>
              
                        </ul>
                    </li>
                    <li><a href="/vouchers"><i class="fa fa-arrow-left"></i> <span class="nav-label">Salidas</span></a></li>
                    <li><a href="/devolutions"><i class="fa fa-arrow-left"></i> <span class="nav-label">Devoluciones</span></a></li>
