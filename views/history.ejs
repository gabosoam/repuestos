                $.get("/history/read/<%= product.id %>", function (data2) {
                    console.log($('#Vouchers'))

                    w3.displayObject("Vouchers", { voucherss: data2 });

                });

                function print() {
                    $('#print').printArea();
                }
            </script>




</body>

</html>

<div class="modal fade" id="modalReporte" role="dialog">
    <div class="modal-dialog modal-lg">

